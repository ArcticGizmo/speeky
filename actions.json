{
  "actionStart": "hi speeky",
  "actions": [
    {
      "name": "What is",
      "pattern": ["what is", "::(.*)"],
      "module": "speeky.actions",
      "function": "whatIs"
    },
    {
      "name": "Say",
      "pattern": ["say", "::(.*)"],
      "module": "speeky.actions",
      "function": "say"
    },
    {
      "name": "Calculate",
      "pattern": ["calculate", "::(.*)"],
      "module": "speeky.actions",
      "function": "calculate"
    },
    {
      "name": "Text numbers to int",
      "pattern": ["::(.*)", "(as|to) (int|integer|number)"],
      "module": "speeky.actions",
      "function": "text2Number"
    }
  ]
}